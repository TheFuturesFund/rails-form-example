<p id="notice"><%= notice %></p>

<h1>Forms</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email address</th>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Zipcode</th>
      <th>Gender</th>
      <th>Grade</th>
      <th>Dietary restrictions</th>
      <th>Dietary restrictions description</th>
      <th>Emergency contact name</th>
      <th>Emergency contact relationship</th>
      <th>Emergency contact phone</th>
      <th>Emergency contact address</th>
      <th>Emergency contact city</th>
      <th>Emergency contact state</th>
      <th>Emergency contact zipcode</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @forms.each do |form| %>
      <tr>
        <td><%= form.first_name %></td>
        <td><%= form.last_name %></td>
        <td><%= form.email_address %></td>
        <td><%= form.address %></td>
        <td><%= form.city %></td>
        <td><%= form.state %></td>
        <td><%= form.zipcode %></td>
        <td><%= form.gender %></td>
        <td><%= form.grade %></td>
        <td><%= form.dietary_restrictions %></td>
        <td><%= form.dietary_restrictions_description %></td>
        <td><%= form.emergency_contact_name %></td>
        <td><%= form.emergency_contact_relationship %></td>
        <td><%= form.emergency_contact_phone %></td>
        <td><%= form.emergency_contact_address %></td>
        <td><%= form.emergency_contact_city %></td>
        <td><%= form.emergency_contact_state %></td>
        <td><%= form.emergency_contact_zipcode %></td>
        <td><%= link_to 'Show', form %></td>
        <td><%= link_to 'Edit', edit_form_path(form) %></td>
        <td><%= link_to 'Destroy', form, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Form', new_form_path %>
